<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ajn.springboot.studyspringboot.mapper.UserMapper">

    <select id="selectByUsername" resultType="com.ajn.springboot.studyspringboot.model.User">
        SELECT *
        FROM admin
        WHERE username = #{username}
    </select>

</mapper>